{
  "name": "currency-conversion",
  "packageManager": "pnpm@7.29.1",
  "scripts": {
    "dev": "vite --host & npm run style:watch",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "prepare": "husky install",
    "style:watch": "typed-scss-modules src --watch --ignore \"**/index.scss\""
  },
  "dependencies": {
    "@vercel/analytics": "^0.1.11",
    "chart.js": "^4.2.1",
    "dayjs": "^1.11.7",
    "mathjs": "^11.6.0",
    "reset-css": "^5.0.1",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@lmmmmmm/eslint-config": "^0.0.3",
    "@lmmmmmm/eslint-config-vue": "^0.0.3",
    "@types/node": "^18.15.1",
    "@vicons/carbon": "^0.12.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "naive-ui": "^2.34.3",
    "sass": "^1.59.2",
    "typed-scss-modules": "^7.0.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-open-graph": "^1.1.5",
    "vue-tsc": "^1.2.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": "eslint . --fix"
  }
}

{
  "name": "currency-conversion",
  "packageManager": "pnpm@8.3.1",
  "scripts": {
    "dev": "vite --host & npm run style:watch",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "prepare": "husky install",
    "style:watch": "typed-scss-modules src --watch --ignore \"**/index.scss\""
  },
  "dependencies": {
    "@vercel/analytics": "^1.0.0",
    "chart.js": "^4.3.0",
    "dayjs": "^1.11.7",
    "mathjs": "^11.8.0",
    "reset-css": "^5.0.1",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@lmmmmmm/eslint-config": "^0.1.0",
    "@lmmmmmm/eslint-config-vue": "^0.1.0",
    "@types/node": "^18.16.2",
    "@vicons/carbon": "^0.12.0",
    "@vitejs/plugin-vue": "^4.2.1",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "naive-ui": "^2.34.3",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.1",
    "typed-scss-modules": "^7.1.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
    "vite-plugin-open-graph": "^1.1.5",
    "vue-tsc": "^1.6.1"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": "eslint . --fix"
  }
}

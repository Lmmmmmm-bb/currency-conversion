{
  "name": "currency-conversion",
  "packageManager": "pnpm@8.7.4",
  "scripts": {
    "dev": "vite --host & npm run style:watch",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "prepare": "husky install",
    "style:watch": "typed-scss-modules src --watch --ignore \"**/index.scss\""
  },
  "dependencies": {
    "@vercel/analytics": "^1.0.2",
    "chart.js": "^4.4.0",
    "dayjs": "^1.11.9",
    "mathjs": "^11.11.0",
    "reset-css": "^5.0.2",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@lmmmmmm/eslint-config": "^0.3.0",
    "@lmmmmmm/eslint-config-vue": "^0.3.0",
    "@types/node": "^18.17.15",
    "@vicons/carbon": "^0.12.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "eslint": "^8.49.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "naive-ui": "^2.34.4",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.66.1",
    "typed-scss-modules": "^7.1.4",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-open-graph": "^2.0.3",
    "vue-tsc": "^1.8.10"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": "eslint . --fix"
  }
}
